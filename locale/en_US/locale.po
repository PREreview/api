msgid "plugins.generic.prereview.title"
msgstr "PREreview"

msgid "plugins.generic.prereview.description"
msgstr "This plugin allows you to give your authors the option to request constructive feedback on their preprint from the PREreview research community (https://prereview.org/reviews)."

msgid "plugins.generic.prereview.apisettings"
msgstr "Configure access to the PREreview API to request reviews from the PREreview community."

msgid "plugins.generic.prereview.apisettings.title"
msgstr "API Keys"

msgid "plugins.generic.prereview.apisettings.name"
msgstr "API" 

msgid "plugins.generic.prereview.apisettings.key"
msgstr "KEY"

msgid "plugins.generic.prereview.apisettings.option"
msgstr "PREreview display configuration"

msgid "plugins.generic.prereview.apisettings.optionDescription"
msgstr "On PREreview, community reviewers can provide rapid and structured feedback in the form of a Rapid PREreview (e.g., ___), and a longer free-formed review, a Full PREreview (e.g., ___)."

msgid "plugins.generic.prereview.apisettings.optionDescription.ask"
msgstr "What reviews from the PREreview website would you like to be displayed near the preprint?"

msgid "plugins.generic.prereview.apisettings.showrevisions"
msgstr "Display Rapid PREreviews only"

msgid "plugins.generic.prereview.apisettings.showrevisionsLong"
msgstr "Display Full PREreviews only"

msgid "plugins.generic.prereview.apisettings.showrevisionsBoth"
msgstr "Display both Rapid PREreviews and Full PREreviews"

